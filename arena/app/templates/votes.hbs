<table class="table">
<tr>
    <th>Player</th>
    <th>Game</th>
    <th>Vote</th>
</tr>
{{#each}}
<tr>
    <td>{{#link-to 'player' this.player}}{{quake-name player.name}}{{/link-to}}</td>
    <td>{{#link-to 'game' this.game}}{{game.mapname}}{{/link-to}}</td>
    <td>{{vote}}</td>
</tr>
{{/each}}
</table>